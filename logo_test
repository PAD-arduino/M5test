uint16_t colorred = TFT_RED;
uint16_t colorblue = TFT_BLUE;
uint16_t colorstar = TFT_WHITE;
uint16_t color = TFT_WHITE;

void setup() {
  M5.begin();
  M5.Lcd.setRotation(1);
  M5.Lcd.fillScreen(TFT_BLACK);

  M5.Lcd.drawXBitmap(logoX, logoY, tamiyared, logoredWidth, logoredHeight, colorred);
  M5.Lcd.drawXBitmap(logoX, logoY, tamiyablue, logoblueWidth, logoblueHeight, colorblue);
  M5.Lcd.drawXBitmap(logoX, logoY, star, tamiyaWidth, tamiyaHeight, colorstar);
  M5.Lcd.drawXBitmap(logoX, logoY, tamiya, tamiyaWidth, tamiyaHeight, color);
  M5.Lcd.setCursor(60, 34);
  M5.Lcd.setTextSize(2);
  M5.Lcd.setTextColor(color);
}

void loop() {
  M5.update();

  if (M5.BtnA.isPressed()) {
    switch (color) {
      case TFT_WHITE:
        color = TFT_GREEN;
        break;
      case TFT_GREEN:
        color = TFT_BLUE;
        break;
      case TFT_BLUE:
        color = TFT_WHITE;
        break;
    }

    M5.Lcd.drawXBitmap(logoX, logoY, tamiya, tamiyaWidth, tamiyaHeight, color);
    delay(200);
  }
}
